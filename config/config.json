{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "todo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/todo/TodoWebPart.js",
          "manifest": "./src/webparts/todo/TodoWebPart.manifest.json"
        }
      ]
    },
    "todo-list-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/todoList/TodoListFieldCustomizer.js",
          "manifest": "./src/extensions/todoList/TodoListFieldCustomizer.manifest.json"
        }
      ]
    },
    "todo-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/todo/TodoCommandSet.js",
          "manifest": "./src/extensions/todo/TodoCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "TodoWebPartStrings": "lib/webparts/todo/loc/{locale}.js",
    "TodoListFieldCustomizerStrings": "lib/extensions/todoList/loc/{locale}.js",
    "TodoCommandSetStrings": "lib/extensions/todo/loc/{locale}.js"
  }
}
